{
  "name": "@filefeed/react",
  "version": "0.1.3",
  "description": "A powerful data onboarding SDK for importing, mapping, validating, and transforming structured data",
  "main": "dist/client.js",
  "module": "dist/client.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/client.esm.js",
      "require": "./dist/client.js"
    }
  },
  "files": [
    "dist/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": true,
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build-lib": "tsc -p tsconfig.lib.json && tsc-alias -p tsconfig.lib.json && rollup -c",
    "prepublishOnly": "npm run clean && npm run build-lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FileFeed/filefeed-react.git"
  },
  "homepage": "https://github.com/FileFeed/filefeed-react#readme",
  "bugs": {
    "url": "https://github.com/FileFeed/filefeed-react/issues"
  },
  "keywords": [
    "data-onboarding",
    "csv-import",
    "excel-import",
    "data-validation",
    "data-mapping",
    "sdk",
    "react",
    "typescript"
  ],
  "author": "Filefeed Team",
  "license": "MIT",
  "dependencies": {
    "jschardet": "^3.1.4",
    "papaparse": "^5.4.1",
    "xlsx": "^0.18.5",
    "zustand": "^4.4.7",
    "@mantine/core": "^7.3.0",
    "@tabler/icons-react": "^2.44.0"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.10",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.8.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.31",
    "rollup": "^4.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
